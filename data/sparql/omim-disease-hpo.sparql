 PREFIX ov: <http://bio2rdf.org/openpredict_vocabulary:>
 PREFIX dct: <http://purl.org/dc/terms/>
 SELECT distinct ?diseaseid ?hpoid
 {
  #?drug ov:indication ?disease .
  ?disease <http://semanticscience.org/resource/SIO_001279> ?hpo
  BIND( STRAFTER(str(?disease),"http://identifiers.org/mim/") AS ?diseaseid)
  BIND( STRAFTER(str(?hpo),"http://identifiers.org/hp/") AS ?hpoid)
}

